<script type="text/javascript">
    $(function () {
        $('.navbar-inverse  a[nav]').each(function () {
            $this = $(this);
            if ($this[0].href == String(window.location)) {
                $this.closest('li').addClass("active");
            }
        });
    });
    function closeWindow(){
        window.opener=null;
        window.open('','_self');
        window.close();
    }
</script>
